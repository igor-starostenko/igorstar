  "author": "Igor Starostenko <contact@igorstar.com>",
  "scripts": {
    "dev": "next dev",
    "start": "next start",
    "ssr": "npm run build && npm run start",
    "format": "prettier --config ./.prettierrc --write src/**/*.js",
    "build:dir": "mkdirp ./public/icons/",
    "build:favicons": "node scripts/favicons",
    "build:sitemap": "node scripts/post-export",
    "build": "npm run build:dir && npm run build:favicons && npm run build:sitemap && next build",
    "contentful:backup": "node scripts/contentful_backup.js",
    "contentful:export": "contentful space export --space-id $CONTENTFUL_SPACE_ID",
    "test:lighthouse": "npx ava scripts/lighthouse.test.js"
  },
  "greenkeeper": {
    "ignore": [
      "core-js"
    ]
  },
  "engines": {
    "node": ">=17.0.1"
  },
  "dependencies": {
    "@contentful/rich-text-react-renderer": "^15.6.2",
    "babel-plugin-styled-components": "^1.13.3",
    "contentful": "^9.1.4",
    "dotenv": "^10.0.0",
    "framer-motion": "^5.2.1",
    "next": "^12.0.3",
    "prop-types": "^15.6.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0",
    "react-images": "^1.2.0-beta.7",
    "react-photo-gallery": "^8.0.0",
    "styled-components": "^5.3.3"
  },
  "devDependencies": {
    "ava": "^3.15.0",
    "babel-eslint": "^10.1.0",
    "chrome-launcher": "^0.14.2",
    "contentful-cli": "^1.9.31",
    "contentful-management": "^7.45.0",
    "contentful-migration": "^4.5.9",
    "core-js": "^3.19.1",
    "directory-named-webpack-plugin": "^4.0.0",
    "dotenv-webpack": "^7.0.3",
    "eslint": "^8.2.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.26.1",
    "favicons": "^6.2.2",
    "lighthouse": "^8.6.0",
    "mkdirp": "^1.0.4",
    "node-gyp": "^8.4.0",
    "prettier": "^2.4.1",
    "webpack": "^5.62.1"
  }
}
